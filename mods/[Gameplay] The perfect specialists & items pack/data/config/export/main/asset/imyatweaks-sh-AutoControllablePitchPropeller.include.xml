<ModOps>

    <ImyaTweaks Title="SH - Auto-Controllable-Pitch Propeller" />

    <!-- Auto-Controllable-Pitch Propeller 1742000190-1742000191 -->

    <ModOp GUID="1742000190" Type="replace" Path="/Values/VehicleUpgrade/ForwardSpeedUpgrade/Value" ModOpID="1742000190_ForwardSpeedUpgrade" Skip="1">
        <Value>35</Value>
    </ModOp>

    <ImyaExpose Path="self::Value" ModOpID="1742000190_ForwardSpeedUpgrade" Kind="Slider" ExposeID="Movement Speed (%)">
        <SliderDefinition Min="-100" Max="200" Stepping="1" />
    </ImyaExpose>

    <ModOp GUID="1742000190" Type="replace" Path="/Values/VehicleUpgrade/IgnoreWeightFactorUpgrade/Value" ModOpID="1742000190_IgnoreWeightFactorUpgrade" Skip="1">
        <Value>80</Value>
    </ModOp>

    <ImyaExpose Path="self::Value" ModOpID="1742000190_IgnoreWeightFactorUpgrade" Kind="Slider" ExposeID="Cargo Slowdown (%)" Description="% of Cargo Slowdown. 20% will be ingame Cargo Slowdown reduced with -20%.">
        <SliderDefinition Min="-100" Max="100" Stepping="1" />
    </ImyaExpose>

    <ModOp GUID="1742000190" Type="replace" Path="/Values/VehicleUpgrade/IgnoreDamageFactorUpgrade/Value" ModOpID="1742000190_IgnoreDamageFactorUpgrade" Skip="1">
        <Value>80</Value>
    </ModOp>

    <ImyaExpose Path="self::Value" ModOpID="1742000190_IgnoreDamageFactorUpgrade" Kind="Slider" ExposeID="Damage Slowdown (%)" Description="% of Damage Slowdown. 20% will be ingame Damage Slowdown reduced with -20%.">
        <SliderDefinition Min="-100" Max="100" Stepping="1" />
    </ImyaExpose>

</ModOps>